mixin arrow()
	.swiper-button-hand.swiper-button-hand-prev.swiper-button-prev
		+icon('chevron-left')
	.swiper-button-hand.swiper-button-hand-next.swiper-button-next
		+icon('chevron-right')
//- /arrow for swiper-slider

mixin ie11()
	//- modernizer and for ie11
	 
	<!--[if lt IE 11]><p   class="browsehappy container">К сожалению, вы используете устаревший браузер. Пожалуйста, <a href="http://browsehappy.com/" target="_blank">обновите ваш браузер</a>, чтобы улучшить производительность, качество отображаемого материала и повысить безопасность.</p><![endif]-->


mixin page-title(title, ar)
	+b.page-title
		.container.container-large
			.row
				if ar
					.col-auto
						+e.link(href="#") 
							+icon("arrow-back")
				.col
					if title
						h1= title
				block
			
mixin reg(b="Отправить")
	+form-wrap().form-wrap--sm
		.row
			.col-lg-6
				+input("",'text','Фамилия')(required class="form-control-sm")
			.col-lg-6
				+input("",'text','Имя')(required class="form-control-sm")
			.col-lg-6
				+input("",'text','Отчество')(required class="form-control-sm")
			.col-lg-6
				+input("",'text','Аккаунт в Telegram')(required class="form-control-sm")
					+e.smallest Необязательно
			.col-lg-6
				+input("+7 ___ ___ __ __",'tel','Телефон*')(required class="form-control-sm")
			.col-lg-6
				+input("",'email','E-mail')(required class="form-control-sm")
			.col-lg-6
				+input("",'text','Город')(required class="form-control-sm")
			.w-100 
			.col-lg-6
				button.btn.btn-outline-gradient.w-100.mb-4(type="submit")= b
			.col-lg-6.align-self-center
					+in-c()(checked)
						+e.SPAN.text.small Соглашаюсь на обработку  #[a.text-warning(href="#") персональных данных] 


mixin login(b="Зарегистрироваться")
	+b.form-wrap(class="form-wrap--border")
		.container 
			form(action='/file-upload'  enctype="multipart/form-data")
				//- .dropzone(action='/file-upload'  enctype="multipart/form-data")
				.col-xxl-6.col-lg-8.g-0
					+input("",'email',' E-mail') 
					+input("",'password','Пароль') 
						//- .col-lg-6.mb-4
					+e.btn.w-100.ttu.mb-4(type="submit") Войти
					.h5.py-4 Нет аккаунта? 
						| 
						a(href="#") Зарегистрироваться
					.h5.pb-2
						a(href="#") Восстановить пароль

	 
mixin password(b="Зарегистрироваться")
	+b.form-wrap(class="form-wrap--border")
		.container 
			form(action='/file-upload'  enctype="multipart/form-data")
				//- .dropzone(action='/file-upload'  enctype="multipart/form-data")
				.col-lg-6.g-0
					+input("",'email','E-mail') 
					+e.btn.w-100.ttu(type="submit") Восстановить


